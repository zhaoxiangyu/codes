<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
<application android:icon="@drawable/icon" android:label="@string/app_name" android:description="@string/app_description" android:theme="@style/JpVocreaderTheme">
        <activity android:name="org.sharp.vocreader.view.SplashActi" android:label="@string/app_name" android:icon="@drawable/icon" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.CREATE_SHORTCUT"/>
			</intent-filter>
        </activity>

        <activity android:name="org.sharp.vocreader.view.VocReaderActi" android:label="@string/app_name" android:icon="@drawable/icon" android:screenOrientation="portrait">
        </activity>

        <activity android:name="org.sharp.gpstracker.TrackerUI" android:icon="@drawable/compass" android:label="@string/gpstracker" android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.CREATE_SHORTCUT"/>
			</intent-filter>
        </activity>        

        <activity android:name="org.sharp.android.view.LogFileUI" android:icon="@drawable/log" android:label="@string/logfileview" android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.CREATE_SHORTCUT"/>
			</intent-filter>
        </activity>        

		<activity android:name="com.google.ads.AdActivity" android:configChanges="keyboard|keyboardHidden|orientation"/>
        <!-- youmi -->
		<activity android:name="net.youmi.android.AdActivity" android:configChanges="keyboard|keyboardHidden|orientation"/>
		<activity android:name="net.youmi.android.appoffers.AppOffersActivity" android:configChanges="keyboard|keyboardHidden|orientation"/>
		<!-- umeng -->           
		<meta-data android:value="4ebcd85052701541c6000026" android:name="UMENG_APPKEY"/>
		<meta-data android:value="应用汇" android:name="UMENG_CHANNEL"/>
		<activity android:name="com.feedback.ui.SendFeedback" android:windowSoftInputMode="adjustResize"/> 
		<activity android:name="com.feedback.ui.FeedbackConversations"/> 
		<activity android:name="com.feedback.ui.FeedbackConversation" android:configChanges="keyboard"/>
    </application>
