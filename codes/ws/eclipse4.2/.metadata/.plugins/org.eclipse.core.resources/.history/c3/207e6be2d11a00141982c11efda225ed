package org.sharp.utils.beans;

public class FileInfo {
	String path;
	String name;

	public FileInfo() {

	}

	public FileInfo(String path, String name) {
		this.path = path;
		this.name = name;
	}

	public String getPath() {
		return path;
	}

	public void setPath(String path) {
		this.path = path;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}