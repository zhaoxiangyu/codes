PROGRAMS = main 
RM = /bin/rm
SRC_PATH = . 
INCLUDE = include/
OBJECT_PATH = include/
vpath %.o $(OBJECT_PATH)

OBJECTS	= RtAudio.o kiss_fft.o kiss_fftr.o 

CC       = g++
DEFS     = -DHAVE_GETTIMEOFDAY -D__LINUX_ALSA__
CFLAGS   = -g -O2 -Wall
CFLAGS  += -I$(INCLUDE)
LIBRARY  = -lpthread -lasound -lm 

%.o : /include/%.cpp
	$(CC) $(CFLAGS) $(DEFS) -c $(<) -o $(OBJECT_PATH)/$@

all : $(PROGRAMS)

main : main.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o main main.cpp $(OBJECT_PATH)*.o $(LIBRARY)
	

clean :
	#$(RM) -f $(OBJECT_PATH)/*.o
	#$(RM) -f $(PROGRAMS)
	#$(RM) -f *.raw *~ *.exe
	#$(RM) -fR *.dSYM
	$(RM)	*.txt
	


strip : 
	strip $(PROGRAMS)
